{
  "name": "autodateformat",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "npm run typings install",
    "grunt": "grunt",
    "build": "npm run grunt",
    "tsc": "tsc",
    "tsc:w": "tsc -w --outDir dev --sourceMap",
    "compass:w": "compass watch -c compass.rb -e dev",
    "lite": "lite-server",
    "start": "mkdir -p dev && cp src/index.html dev/index.html && concurrent \"npm run compass:w\" \"npm run tsc:w\" \"npm run lite\" ",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compass": "^1.1.1",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-targethtml": "^0.2.6",
    "grunt-ts": "^5.4.0",
    "jquery": "^2.2.2",
    "jqueryui": "^1.11.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compass": "^1.1.1",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-targethtml": "^0.2.6",
    "grunt-ts": "^5.4.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "uglify": "^0.1.5"
  }
}
