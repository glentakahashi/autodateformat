{
  "name": "autodateformat",
  "version": "1.0.0",
  "main": "build/app.js",
  "style": "build/app.css",
  "scripts": {
    "build": "webpack --config ./config/webpack.config.js",
    "build:dev": "tsc -p .",
    "check": "npm-run-all lint",
    "clean": "rimraf dist/*",
    "copy:index": "cp src/index.html build/src",
    "dist": "NODE_ENV=production npm-run-all build dist:sls",
    "dist:sls": "node-sls-packaging",
    "dev": "npm-run-all -p -c build:dev copy:index dev-server watch",
    "dev-server": "webpack-dev-server --https --hot --config ./config/webpack.config.js",
    "lint": "npm-run-all -p lint:stylelint lint:tslint",
    "lint:stylelint": "stylelint -s scss src/**/*.scss",
    "lint:tslint": "tslint -c config/tslint.json 'src/**/*.{ts,tsx}'",
    "lint:tslint:fix": "tslint --fix -c config/tslint.json 'src/**/*.{ts,tsx}'",
    "verify": "npm-run-all clean build lint",
    "watch": "yarn build -- --watch"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.24.0",
    "@blueprintjs/labs": "^0.5.0",
    "es6-shim": "^0.35.0",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "typescript-fsa": "^2.3.1",
    "typescript-fsa-reducers": "^0.4.4"
  },
  "devDependencies": {
    "@types/history": "^4.6.0",
    "@types/lodash": "^4.14.72",
    "@types/moment-timezone": "0.2.34",
    "@types/react": "^15.6.1",
    "@types/react-dom": "^15.5.2",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "^4.0.14",
    "@types/react-router-redux": "^5.0.5",
    "@types/react-select": "^1.0.51",
    "@types/redux-logger": "^3.0.0",
    "autoprefixer": "^7.1.2",
    "circular-dependency-plugin": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "tslint": "^5.6.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-notifier": "^1.5.0"
  },
  "resolutions": {
    "@types/react": "^15.6.1"
  }
}
