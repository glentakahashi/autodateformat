{
  "name": "autodateformat",
  "version": "1.0.0",
  "main": "build/app.js",
  "style": "build/app.css",
  "scripts": {
    "build": "webpack --config ./config/webpack.config.js",
    "build:dev": "tsc -p .",
    "check": "npm-run-all lint",
    "clean": "rimraf dist/*",
    "copy:index": "cp src/index.html build/src",
    "dist": "NODE_ENV=production npm-run-all build dist:sls",
    "dist:sls": "node-sls-packaging",
    "dev": "npm-run-all -p -c build:dev copy:index dev-server watch",
    "dev-server": "webpack-dev-server --https --hot --config ./config/webpack.config.js",
    "lint": "npm-run-all -p lint:stylelint lint:tslint",
    "lint:stylelint": "stylelint -s scss src/**/*.scss",
    "lint:tslint": "tslint -c config/tslint.json 'src/**/*.{ts,tsx}'",
    "lint:tslint:fix": "tslint --fix -c config/tslint.json 'src/**/*.{ts,tsx}'",
    "verify": "npm-run-all clean build lint",
    "watch": "yarn build -- --watch"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.17.0",
    "@blueprintjs/labs": "^0.3.0",
    "@types/history": "2.0.45",
    "@types/lodash": "4.14.50",
    "@types/moment-timezone": "0.2.34",
    "@types/react": "15.0.5",
    "@types/react-dom": "0.14.22",
    "@types/react-redux": "4.4.36",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "4.0.36",
    "@types/react-select": "1.0.40",
    "@types/redux-logger": "^3.0.0",
    "es6-shim": "^0.35.0",
    "history": "3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "react-select": "1.0.0-rc.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "tslint-blueprint": "^0.0.1",
    "typescript-fsa": "^2.3.1",
    "typescript-fsa-reducers": "^0.4.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "circular-dependency-plugin": "^2.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "jest": "^19.0.2",
    "node-sass": "4.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "1.2.1",
    "postcss-scss": "0.4.0",
    "sass-loader": "4.1.0",
    "source-map-loader": "0.1.5",
    "tslint": "^5.5.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "webpack": "3.0.0",
    "webpack-dev-server": "1.16.2",
    "webpack-notifier": "^1.5.0"
  }
}
